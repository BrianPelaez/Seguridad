	}
	
	SALIDA
	{
		nombre:
		apellido:
		dni:
	}
	
###		MODIFICAR ALUMNO	###
api/alumno/{id}

	POST
	ENTRADA
	{
		nombre:
		apellido:
		dni:
		borrado:
	}
	
	SALIDA
	{
		nombre:
		apellido:
		dni:
	}
	
###		BORRAR ALUMNO		###
api/alumno/{id}

